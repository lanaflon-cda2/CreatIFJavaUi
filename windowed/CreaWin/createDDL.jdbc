CREATE TABLE ARTISTE (IDARTISTE INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, PRENOM VARCHAR(255), COTE INTEGER, NOM VARCHAR(255), BIO VARCHAR(255), PRIMARY KEY (IDARTISTE))
CREATE TABLE CLIENT (NUMCLIENT INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, PRENOM VARCHAR(255), ADRESSE VARCHAR(255), NOM VARCHAR(255), NUMTEL VARCHAR(255), PRIMARY KEY (NUMCLIENT))
CREATE TABLE GALERIE (IDGALERIE INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, DATERETOUR DATE, DATEDEBUT DATE, CLIENT_NUMCLIENT INTEGER, PRIMARY KEY (IDGALERIE))
CREATE TABLE GALERIE_OEUVRE (listeOeuvre_IDOEUVRE INTEGER NOT NULL, listeGalerie_IDGALERIE INTEGER NOT NULL, PRIMARY KEY (listeOeuvre_IDOEUVRE, listeGalerie_IDGALERIE))
CREATE TABLE OEUVRE (IDOEUVRE INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, DTYPE VARCHAR(31), TARIFJOURNALIER INTEGER, DESCRIPTION VARCHAR(255), NOM VARCHAR(255), ARTISTE_IDARTISTE INTEGER, HAUTEUR INTEGER, LARGEUR INTEGER, EPAISSEUR INTEGER, PRIMARY KEY (IDOEUVRE))
ALTER TABLE GALERIE ADD CONSTRAINT GLRIECLNTNUMCLIENT FOREIGN KEY (CLIENT_NUMCLIENT) REFERENCES CLIENT (NUMCLIENT)
ALTER TABLE GALERIE_OEUVRE ADD CONSTRAINT GLRVRlstGlrDGLERIE FOREIGN KEY (listeGalerie_IDGALERIE) REFERENCES GALERIE (IDGALERIE)
ALTER TABLE GALERIE_OEUVRE ADD CONSTRAINT GLRVRElstvrDOEUVRE FOREIGN KEY (listeOeuvre_IDOEUVRE) REFERENCES OEUVRE (IDOEUVRE)
ALTER TABLE OEUVRE ADD CONSTRAINT VRERTISTEIDARTISTE FOREIGN KEY (ARTISTE_IDARTISTE) REFERENCES ARTISTE (IDARTISTE)
